            <SplitView.Pane>
                <Border Margin="0,106,0,10" BorderBrush="#66000000" BorderThickness="1,0,0,0">
                    <Border.Background>
                        <AcrylicBrush BackgroundSource="Backdrop"/>
                    </Border.Background>
                    <StackPanel Spacing="20" Margin="10">
                        <TextBlock Text="选择查询时间" FontSize="24" Margin="0,10,0,0" Foreground="{StaticResource AccentText}"/>
                        <CalendarView x:Name="Calendar" Language="zh" IsGroupLabelVisible="True" VerticalAlignment="Top" OutOfScopeForeground="Gray">
                            <CalendarView.OutOfScopeBackground>
                                <AcrylicBrush BackgroundSource="HostBackdrop" FallbackColor="#FFE8E8E8" TintColor="#CCE8E8E8"/>
                            </CalendarView.OutOfScopeBackground>
                            <CalendarView.CalendarItemBackground>
                                <AcrylicBrush BackgroundSource="HostBackdrop" FallbackColor="#FFFFFFFF" TintColor="#DDFFFFFF"/>
                            </CalendarView.CalendarItemBackground>
                            <CalendarView.BorderBrush>
                                <AcrylicBrush BackgroundSource="HostBackdrop" FallbackColor="#FFD0D0D0" TintColor="#CCD0D0D0"/>
                            </CalendarView.BorderBrush>
                        </CalendarView>
                        <StackPanel Orientation="Horizontal" Spacing="9">
                            <TimePicker x:Name="TimePicker" ClockIdentifier="24HourClock"/>
                            <Button Content="现在" Click="Now_C"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="10">
                            <TextBlock Text="当前查询时间" FontSize="16" Foreground="{StaticResource AccentText}" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                            <TextBlock x:Name="currentCutoff" Text="2023/6/13 17:17 +08:00" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                        </StackPanel>

                        <Button Content="更新时间" HorizontalAlignment="Stretch" VerticalAlignment="Center" Click="Submit"/>
                        <TextBlock Foreground="{StaticResource SoftText}" TextWrapping="Wrap" FontSize="12"><Run Text="● 极值统计数据采用8-8数据，日低温除外，为方便统计，日低温数据采用20-20数据。由于自动站数据回传需要时间，因此所有数据额定更新4分钟以前的结果；"/><LineBreak/><LineBreak/><Run Text="● “广东近岸洋面”模式较为特殊，每日0:00极值统计数据自动清空，且浮标站和平台站数据回传时间较长，通常超过额定4分钟时间，导致无法获取最新一轮数据。必要时请更新时间至9分钟以前；"/><LineBreak/><LineBreak/><Run Text="● “广东近岸洋面”模式没有降水数据，请切换至“广东沿海城市”模式查看。"/></TextBlock>
                    </StackPanel>

                </Border>
            </SplitView.Pane>
        </SplitView>

        <StackPanel Orientation="Horizontal" Spacing="10" Margin="0,52,20,0" HorizontalAlignment="Right" VerticalAlignment="Top">
            <TextBlock Text="区域" VerticalAlignment="Center" FontWeight="Bold" Foreground="{ThemeResource SystemControlBackgroundBaseMediumRevealBorderBrush}"/>
            <!--<ComboBox x:Name="region" PlaceholderText="选择查询模式" SelectedIndex="0" BorderThickness="1" BorderBrush="{ThemeResource SystemControlBackgroundBaseLowRevealBorderBrush}" Width="128" SelectionChanged="CB_SC" VerticalAlignment="Center">
                <x:String>广州</x:String>
                <x:String>广东沿海城市</x:String>
                <x:String>广东近岸洋面</x:String>
            </ComboBox>-->
            <ComboBox x:Name="region" SelectedItem="{Binding selectedRegion}" ItemsSource="{Binding regions}" DisplayMemberPath="Name" SelectedValuePath="Code" PlaceholderText="选择查询区域" BorderThickness="1" BorderBrush="{ThemeResource SystemControlBackgroundBaseLowRevealBorderBrush}" Width="128" SelectionChanged="CB_SC" VerticalAlignment="Center">
                <ComboBox.Tag>
                    <x:Boolean>false</x:Boolean>
                </ComboBox.Tag>
            </ComboBox>
            <AppBarSeparator Margin="-10,0" Foreground="{ThemeResource SystemControlBackgroundBaseMediumRevealBorderBrush}"/>
            <TextBlock Text="实况序列" VerticalAlignment="Center" FontWeight="Bold" Foreground="{ThemeResource SystemControlBackgroundBaseMediumRevealBorderBrush}"/>
            <ToggleSwitch x:Name="autoQueryDetails" IsOn="True" OnContent="自动" OffContent="关闭" Width="90" Toggled="autoQueryDetails_Toggled"/>
            <StackPanel x:Name="cancelSp" Visibility="Collapsed" Orientation="Horizontal">
                <AppBarSeparator Margin="-10,0,0,0" Foreground="{ThemeResource SystemControlBackgroundBaseMediumRevealBorderBrush}"/>
                <Button x:Name="Cancel" BorderBrush="{ThemeResource SystemControlBackgroundBaseLowRevealBorderBrush}" BorderThickness="1" Click="Cancel_C">
                    <StackPanel Orientation="Horizontal">
                        <SymbolIcon Symbol="pause"/>
                        <TextBlock x:Name="CancelHint" Text="终止查询"/>
                    </StackPanel>
                </Button>
            </StackPanel>
            <AppBarSeparator Margin="-10,0" Foreground="{ThemeResource SystemControlBackgroundBaseMediumRevealBorderBrush}"/>
            <Button x:Name="paneBtn" Background="Transparent" BorderBrush="{ThemeResource SystemControlTransparentRevealBorderBrush}" BorderThickness="1" Click="paneBtn_C">
                <SymbolIcon Symbol="Setting"/>
            </Button>
        </StackPanel>

        <Popup x:Name="notification" IsOpen="False" VerticalAlignment="Top" Margin="533,52,0,0" HorizontalAlignment="Right">
            <Border x:Name="notifyBd" Background="#FFF0F0F0" BorderBrush="#FFC4C4C4" BorderThickness="1" PointerPressed="notifyBd_PointerPressed">
                <Grid>
                    <StackPanel Margin="10" Spacing="10" Orientation="Horizontal">
                        <Border Background="DodgerBlue" Width="18" Height="18" CornerRadius="9" BorderThickness="0">
                            <TextBlock Text="i" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White" FontSize="10"/>
                        </Border>
                        <TextBlock x:Name="notifyText" Text="自动站时间序列加载完成" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </StackPanel>
                    <Border Width="5" Background="{StaticResource AccentText}" HorizontalAlignment="Right"/>
                </Grid>
            </Border>
            <Popup.ChildTransitions>
                <TransitionCollection>
                    <PopupThemeTransition/>
                </TransitionCollection>
            </Popup.ChildTransitions>
        </Popup>
    </Grid>
</Page>
